<head>
  <title>Jqmoblog</title>
  <meta name="viewport" content="width=device-width, initial-scale=1"> 
  <%= stylesheet_link_tag("http://code.jquery.com/mobile/1.0/jquery.mobile-1.0.min.css") %>
  <%= stylesheet_link_tag("profiles.css") %>
  <%= javascript_include_tag "http://code.jquery.com/jquery-1.6.4.min.js" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "jquery_mobile" %>
  <%= csrf_meta_tag %>

<script type="text/javascript">

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-27407053-1']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>

</head>
<body>
  <%= yield %>
</body>
